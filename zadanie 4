#include <iostream>
#include <time.h>
using namespace std;

int main()
{
	int n;
	srand(time(NULL));
	cout << "podaj wielkość tablicy?" << endl;
	cin >> n;
	int* tab= new int[n];
	for (int i = 0; i < n; i++) {
		tab[i] = rand() % 84 - 23;
	}
	for (int i = 0; i < n; i++) {
		cout << tab[i] << endl;
	}
	
}

